<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <shape
        android:shape="rectangle">
        <gradient
            android:startColor="#86CEE4"
            android:endColor="#9F9ECD"
            android:angle="180" />
        <corners android:radius="15dp"/>
    </shape>
</item>
<item
    android:bottom="1dp"
    android:left="1dp"
    android:right="1dp"
    android:top="1dp">
    <shape android:shape="rectangle">
        <gradient
            android:startColor="#252432"
            android:endColor="#414650"
            android:angle="315"/>
        <corners android:radius="15dp"/>
    </shape>
</item>
</layer-list>